#include <iostream>

using namespace std;

int main()
{
    int days;
    float fine;
    
    cout << "Enter the Days : ";
    cin >> days;
    
    if (days > 0 && days <= 5)
    {
        fine = 0.5 * days;
        cout << "Your Late Fee is : " << fine;
    }
    else if (days > 5 && days <= 10)
    {
        fine = 1.0 * days;
        cout << "Your Late Fee is : " << fine;
    }
    else if ( days > 10 && days < 30)
    {
        fine = 5.0 * days;
        cout << "Your Late Fee is : " << fine;
    }
    else
    cout << "Your Membership is Cancelled";
    
    
    return 0;
}
