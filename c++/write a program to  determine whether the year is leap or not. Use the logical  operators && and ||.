#include <iostream>

using namespace std;

int main()
{
    int year;
    cout << "Enter the Year : ";
    cin >> year;
    
    if ((year % 4 == 0 || year % 400 == 0 ) && ( year % 100 !=0))
    {
        cout << year << " is Leap Year";
    }
    else
        cout << year << " is not a Leap Year";

    return 0;
}
