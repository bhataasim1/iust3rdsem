#include <stdio.h>

int main()
{
   int amont;
   printf("Enter Amont in Humdreds : ",amont);
   scanf("%d",&amont);
   
   printf("100 doms : %d\n", amont/100);
   printf("50 doms : %d\n", (amont % 100) / 50);
   printf("10 doms : %d\n", (amont % 100) % 50 / 10);
   
   printf("Remaning doms : %d\n", (amont % 100) % 50 % 10);
    return 0;
}
