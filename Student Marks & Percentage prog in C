#include <stdio.h>

int main ()
{
  int b1,b2,b3,b4,b5;
  float total,percentage;
  
  printf("Enter the marks in all books ",b1,b2,b3,b4,b5);
  scanf("%d %d %d %d %d",&b1,&b2,&b3,&b4,&b5);
  
  total=b1+b2+b3+b4+b5;
  printf("Total Marks in all Subjects is %f ",total);
  
  percentage=(total/500)*100;
  printf("Percentage is %f ",percentage);
  return 0;
}
