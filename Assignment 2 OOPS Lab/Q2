#include <iostream>

using namespace std;

int main()
{
    float weight, height, bmi;
    cout << "Enter your Weight is Pounds : ";
    cin >> weight;
    
    cout << "Enter your Height in Inches : ";
    cin >> height;
    
    bmi = (703 * weight) / (height * height) ;
    cout << "Your BMI is : " << bmi << endl;
    
    if ( bmi <  18.5)
    {
        cout << "You are Underweight";
    }
    else if ( bmi >= 18.5 && bmi <= 24.9)
    {
        cout << "You are Normal";
    }
    else if ( bmi >= 25.0 && bmi <= 29.96)
    {
        cout << "You are Overweight";
    }
    else if ( bmi >= 30.0)
    {
        cout << "You are Obese";
    }
    else
      cout << "Wrong Entry";

    return 0;
}
