#include <stdio.h>

int
main ()
{
  float km,m,ft,in;
  printf("Enter the Distance in Kilometers ",km);
  scanf("%f",&km);
  
  m=km*1000;
  printf("Distance in meters is %f \n",m);
  
  ft=km*3280.8;
  printf("Distance in Feet is %f \n",ft);
  
  in=km*39370.1;
  printf("Distance in Inch is %f \n",in);
  
  return 0;
}
