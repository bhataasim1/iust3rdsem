#include <stdio.h>

int main()
{
   float total=80000, pmen= 0.52, total_literacy_percentage=0.48, litrate_men_percentage=0.35;
   float total_men= total * pmen;
   float total_litrates = total_men *litrate_men_percentage;
   float illetrate_men = total_men - total_litrates;
   float total_literacy = total * total_literacy_percentage;
   float illetrate_women = total_literacy - total_litrates;
   
   printf("total Illetrate men is %f \n Total Illetrate Women is %f", illetrate_men, illetrate_women);
    return 0;
}
